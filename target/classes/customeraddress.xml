<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	<bean id="addressObj"
		class="com.ojas.emp.Springcoreproj.model.Address">
		<constructor-arg value="2-124"></constructor-arg>
		<constructor-arg value="Kranthi Nagar"></constructor-arg>
		<constructor-arg value="Hyderabad"></constructor-arg>
	</bean>

	<bean id="custObj"
		class="com.ojas.emp.Springcoreproj.model.Customer">
		<constructor-arg value="2"></constructor-arg>
		<constructor-arg value="Santhosh"></constructor-arg>
		<constructor-arg>
			<ref bean="addressObj" />
		</constructor-arg>
	</bean>
</beans>